{
  "name": "treblle-express",
  "version": "1.0.0",
  "description": "Express Sdk for Treblle api monitoring and observability",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsup ./src --format cjs,esm --dts",
    "lint": "tsc",
    "test": ""
  },
  "keywords": [
    "node",
    "express",
    "treblle",
    "middleware",
    "sdk"
  ],
  "author": "Emmanuel Ajike",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "node-os-utils": "^1.3.7",
    "stacktracey": "^2.1.8",
    "treblle": "^1.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.1",
    "@types/node-os-utils": "^1.3.2",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "yalc": "1.0.0-pre.53"
  }
}
